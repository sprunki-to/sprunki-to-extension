{
  "manifest_version": 3,
  "name": "sprunki Games",
  "version": "1.0",
  "description": "Browse and play sprunki games collection",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": [],
  "web_accessible_resources": [{
    "resources": ["games_rows.csv"],
    "matches": ["<all_urls>"]
  }]
} 